            <string>/*that works like this:
if turn =1 (that means we not pressed any char yet), the only thing we need to do is set turn=2 if we pressed the correct key
if turn=2, first we check if key pressed is or not the needed key. If we fail, we reset the sequence, else, set turn=3;
if turn=3, we check again the key pressed. If fail, reset, if not, shoot sonic and destroy al objects involved in algorythm.
*/


//--------------------------------Make "dissappear" chars-------------

if turn=3{
if (!keyboard_check_pressed(third) &amp;&amp; keyboard_check_pressed(vk_anykey)&amp;&amp; allow=0) reset(2);
if keyboard_check_pressed(third) with (charParent){instance_destroy();} 
allow=1;
}
if turn=2{
if (!keyboard_check_pressed(second) &amp;&amp; keyboard_check_pressed(vk_anykey)&amp;&amp; allow=0) reset(1);  
if (keyboard_check_pressed(second)){ global.second.visible=0;turn=3;}
allow=1;
}
if turn=1{
if (keyboard_check_pressed(first)&amp;&amp; allow=0){ global.first.visible=0;turn=2;}
allow=1;
}

//----------------------------------------------------------------------



if (!instance_exists(charParent)){
with (objSonic){
seq=0;
y_speed=-10
x_speed=10
alarm[0]=120}
instance_destroy();
}


time-=1;
global.tim-=1



if time=0{
with (charParent) instance_destroy();
with (objSonic){
seq=0
y_speed=0
x_speed=0
alarm[0]=120}
instance_destroy();
}
allow=0;
</string>
