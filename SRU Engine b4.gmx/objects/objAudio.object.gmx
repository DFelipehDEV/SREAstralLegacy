            <kind>1</kind>
            <string>/// Play stage music.
 // First, stop every single sound.
 if global.Death=false
    audio_stop_all();
    
 // Revert the fadeout flag if it's enabled.
    fadeout_flag = false;
    zone_results = false;
    
 // Set the volumes.
    global.bgmvolume = zone_my_bgm_volume;
    global.sfxvolume = zone_my_sfx_volume;   
    
 // Then play the stage music.
 if global.Death=false{
    switch(room)
    {
           case rm_hub1:
           {
                zone_bgm = bgm_cityhub;
                if(audio_is_playing(zone_bgm) == false)
                {
                   aud_play_music(zone_bgm, global.bgmvolume, 1);
                }
                break;
           }
           case rm_testsr:
           {
                zone_bgm = bgmTestZone;
                if(audio_is_playing(zone_bgm) == false)
                {
                   aud_play_music(zone_bgm, global.bgmvolume, 1);
                }
                break;
           }
           default:
           {    
              zone_bgm        = -1;
              zone_bgm_backup = -1;
              break;
           }
    }
    }
  
</string>
