          <argument>
            <kind>1</kind>
            <string>/// Stop music / sounds.
  
 // Exit when the game over is on screen.
    
 // Exit when the results are on screen.


 // Lower the music volume if the life jingle is playing.
    if(instance_exists(objSonic))
    {
       if(fadeout_flag == 0)
       {
          if(audio_is_playing(sndExtraLife))
          {
             global.bgmvolume = 0.00;
          }
          else
          {  
             if(global.bgmvolume &lt; zone_my_bgm_volume)
             {
                global.bgmvolume += 0.01;
             }  
             if(global.sfxvolume &lt; zone_my_sfx_volume)
             {
                global.sfxvolume += 0.01;
             }        
          }   
       }
       else // This is used when the player has died.
       {
         if(global.bgmvolume != 0.00)
          {
             global.bgmvolume -= 0.01;
          }
          else global.bgmvolume = 0.00;
          if(global.sfxvolume != 0.00)
          {
             global.sfxvolume -= 0.01;
          }     
          else global.sfxvolume = 0.00;               
       }
    }

       if(audio_is_playing(zone_bgm) == false &amp;&amp; zone_bgm != -1)
       {
          aud_play_music(zone_bgm, global.bgmvolume, 1);
       }
    
 // Set the volume of the currently playing zone music and invincibility jingle.
    if(audio_is_playing(bgm_invincibility) == false &amp;&amp; audio_is_playing(bgmDarkspine) == false &amp;&amp; audio_is_playing(bgmSuper) == false &amp;&amp; audio_is_playing(bgmHyper) == false)
    {    
       if(audio_is_playing(zone_bgm) &amp;&amp; zone_bgm != -1)
       {
          audio_sound_gain(zone_bgm, global.bgmvolume, 0);  
       }
    }
    else
    {
       if(audio_is_playing(zone_bgm) &amp;&amp; zone_bgm != -1)
       {    
          audio_sound_gain(zone_bgm, 0.00, 0);   
       }  
    }
 
       if(audio_is_playing(bgm_invincibility))
       {
          audio_sound_gain(bgm_invincibility, global.bgmvolume, 0);
       }  
</string>
